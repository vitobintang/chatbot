{
  "version": 2,
  "builds": [
    {
      "src": "./index.html",
      "use": "@vercel/static"
    },
    {
      "src": "**",
      "use": "@vercel/static"
    },
    {
      "src": "./src/**",
      "use": "@vercel/static"
    },
    {
      "src": "./public",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}